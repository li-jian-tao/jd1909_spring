<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xmlns:context="http://www.springframework.org/schema/context"	xsi:schemaLocation="http://www.springframework.org/schema/beans           http://www.springframework.org/schema/beans/spring-beans-4.3.xsd           http://www.springframework.org/schema/context           http://www.springframework.org/schema/context/spring-context-4.3.xsd">    <!-- 给对象中注入集合 -->        <bean id="s1" class="com.briup.bean.Student">    	<!-- list属性设置基本数据类型+字符串值 -->    	<property name="list" value="3,4,5,1,3"></property>    	<!-- 泛型为 Teacher -->    	<property name="set">    		<set>    			<value>tom1</value>    			<value>tom2</value>    			<value>tom3</value>    			<ref bean="tea"/>    		</set>    	</property>    	<property name="map">    		<map>    			<entry key="tom1" value="31"></entry>    			<entry key="tom2" value="87"></entry>    			<entry key="tom3" value="12"></entry>    		</map>    	</property>    	<property name="prop">			<props>				<prop key="driver">value1</prop>				<prop key="url">value2</prop>			</props>    	    	</property>    </bean>       <bean id="tea" class="com.briup.bean.Teacher"></bean>                                                </beans>